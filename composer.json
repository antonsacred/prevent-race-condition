{
  "name": "sacred/prevent-race-condition",
  "description": "This package allows you to lock some part of code to prevent race condition.",
  "keywords": ["race condition","prevent", "lock"],
  "license": "MIT",
  "version": "1.0.2",
  "require": {
    "php": "^8.0",
    "psr/simple-cache": "^2.0|^3.0",
    "psr/cache": "^2.0|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "symfony/cache": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "PreventRaceCondition\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": [
        "tests/"
      ]
    }
  }
}